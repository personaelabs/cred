{
  "include": ["src/**/*.ts", "src/proto/**/*.ts"],
  "compilerOptions": {
    "outDir": "out",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "paths": {
    "@/*": ["./src/*"]
  },
}
